#include <iostream>
using namespace std;
int main()
{
    int mas[100], n;
    cin>>n;
    cout<<"Massiv"<<endl;
    for(int i=0; i<n; i++)
        cin>>mas[i];
    
    for(int i=0; i<n; i++)
        for(int j=i; j<n; j++)
        {
            if(mas[i]%2==mas[j]%2 && mas[j]<=mas[i])
            {
                swap(mas[i], mas[j]);
                //break;
            }
        }   
        
    for(int i=0; i<n; i++)
        cout<<mas[i]<<' ';           
    return 0;
}
