#include <iostream>
using namespace std;

int main()
{
    int mas[100], n, flag(0), step;
    cin>>n;
    cout<<"Massiv"<<endl;
    for(int i=0; i<n; i++)
        cin>>mas[i];
    
    for(int i=0; i<n; i++)
    {
        step=mas[i];        //можно не делать нврн
        while(step>0)
        {
            if(step%2==1)   //цикличeская проверка на степень двойки
                break;      
            step/=2;        //подгтовка к след иттерации
            if(step==0)     //если найдена степень двойки
                flag++;      
        }
    }
    
    if(flag>1)              //если степеней двойки больше чем одна
        cout<<"YES";
    else
        cout<<"NO";

    return 0;
}
