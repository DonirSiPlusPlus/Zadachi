#include <iostream>
#include <math.h>
using namespace std;
int main()
{
    int mas[100], n;
    cin>>n;
    cout<<"Massiv"<<endl;
    for(int i=0; i<n; i++)
        cin>>mas[i];
    
    for(int i=0; i<n; i++)
        for(int j=i; j<n; j++)
            if(fabs(mas[i]%2)==fabs(mas[j]%2) && mas[j]<=mas[i])    //самое маленькое число двигается влево(пузырек)
                swap(mas[i], mas[j]);                               //перестановка элементов одной четности
        
    for(int i=0; i<n; i++)
        cout<<mas[i]<<' ';           
    return 0;
