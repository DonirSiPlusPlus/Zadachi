#include <iostream>
#include <vector> 
#include <stdlib.h>
#include <time.h>
#include <iomanip>
using namespace std;
void sort(vector<int>& v)                       //функция сортировки вектора 
{
    for(int i=1; i<v.size(); i++)                  
	   for(int j=i; j>0 && v[j-1]>v[j]; j--) 
		  swap(v[j-1], v[j]);
}
int main()
{
    setlocale(LC_ALL, "rus");   
    int n, m;
    cin>>n>>m;
    vector <int> a(n), b(m);
    srand(time(NULL));
    for(int i=0; i<a.size(); i++)               //наполнение первого вектора
        a[i]=rand()%199-99;                     //[-99; 99]
    for(int i=0; i<b.size(); i++)               //наполнение второго вектора
        b[i]=rand()%199-99;     
    sort(a); sort(b);                           //сортировка двух векторов
    cout<<"Первый вектор"<<endl;
    for(int i=0; i<a.size(); i++)               //вывод первого вектора
        cout<<setw(5)<<a[i];
    cout<<endl<<"Второй вектор"<<endl;
    for(int i=0; i<b.size(); i++)               //вывод второго вектора
        cout<<setw(5)<<b[i];
    vector <int>:: iterator p=a.begin(), first=b.begin(), last=b.end(); //итераторы для вставки вектора в вектор
    a.insert(p, first, last);                   //склейка двух векторов
    sort(a);                                    //сортировка общего вектора
    /*for(int i=0; i<b.size(); i++)      
    {
        a.push_back(b[i]);   
        for(int j=a.size()-1; j>0 && a[j]<a[j-1]; j--)    //поиск места для нового элемента (сортировка)
            swap(a[j-1], a[j]);
    }  */
	cout<<endl<<"Общий вектор"<<endl;
        for(int i=0; i<a.size(); i++)                      
            cout<<setw(5)<<a[i];
    return 0;
}
