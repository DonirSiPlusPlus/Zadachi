#include <iostream>
using namespace std;
int main()
{
    int mas[100], n, poisk5, flag(0);
    cin>>n;
    cout<<"Massiv"<<endl;
    
    for(int i=0; i<n; i++)
        cin>>mas[i];
        
    for(int i=0; i<n; i++)
    {
        poisk5=mas[i];              //можно не делать нврн
        while(poisk5>0)             //поиск цифры 5 с помощью остаточного деления
        {
            if(poisk5%10==5)        //если цифра в числе равна 5
            {
                cout<<i<<' ';       //вывод номера числа с цифрой 5
                flag++;             
                break;          
            }   
            poisk5/=10;             //подготовка к след иттерации
        }
    }
    
    if(flag==0)                     //если не нашлось чисел
        cout<<"NO";
    return 0;
}
